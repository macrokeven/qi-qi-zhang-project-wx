.nav-wrap {
    position: fixed;
    width: 100%;
    background: #2969FF;
    z-index: 9999999;
}
.nav-logo{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin: 0 20rpx 0 0;
}
.nav-title {
    padding-left: 16px;
    display: flex;
    flex-direction: row;
    align-items: center;
    font-family: PingFang-SC-Regular;
}

.nav-title-back {
    width: 32rpx;
    height: 32rpx;
    flex: 0 0 32rpx;
    margin-right: 4rpx;
}

.nav-title-text {
    font-size: 34rpx;
    color: #fff;
    font-weight: bold;
}
  